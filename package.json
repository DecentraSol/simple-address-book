{
	"name": "decad",
	"version": "0.0.2",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"cypress:open": "cypress open",
		"cy:alice": "cypress --config-file cypress/cy-alice.config.js",
		"cy:bob": "cypress --config-file cypress/cy-bob.config.js",
		"cy:parallel": "cypress-parallel -s cy:bob -t 2",
		"chat": "concurrently npm:cy:alice npm:cy:bob",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"carbon-components-svelte": "^0.81.0",
		"concurrently": "^8.2.2",
		"cypress": "^13.3.3",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.0"
	},
	"type": "module",
	"dependencies": {
		"qrcode-generator": "^1.4.4"
	}
}
